    "source-map-support": "^0.5.10",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-stack-trace-sourcemap": "^1.0.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0"
  },
  "repository": {
    "type": "git",
