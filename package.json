  "description": "Command line utility to sync files from canvas with a local dir",
  "bin": {
    "csync-canvas": "dist/index.js"
  },
  "scripts": {
    "run": "babel src -d dist && node dist/index.js",
    "watch": "babel -w src -d dist"
  },
  "author": "Michael Fine",
  "license": "ISC",
  "preinstall": "npm install .",
  "dependencies": {
